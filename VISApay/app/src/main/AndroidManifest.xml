<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.visapay">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/payment"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>

        <activity android:name=".ScanningActivity"></activity>
        <activity android:name=".StaticObjectDetectionActivity" />
        <activity android:name=".LiveObjectDetectionActivity" />
        <activity android:name=".LiveBarcodeScanningActivity" />
        <activity android:name=".QRCodeActivity" />
        <activity android:name=".Auth.SignupActivity" />
        <activity android:name=".Auth.PhoneAuthActivity" />
        <activity android:name=".Auth.PasswordlessActivity" />
        <activity android:name=".Auth.LoginActivity" />
        <activity android:name=".Auth.GoogleSignInActivity" />
        <activity android:name=".Auth.DifferentAuth" />
        <activity android:name=".Auth.BaseActivity" />
        <activity android:name=".Auth.AnonymousActivity" />
        <activity android:name=".ListViewBarChartActivity" />
        <activity android:name=".SplashActivity" />
        <activity android:name=".settings.SettingsActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".BoardingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>